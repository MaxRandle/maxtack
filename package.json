{
  "private": true,
  "name": "maxtack",
  "packageManager": "npm@9.2.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:web": "turbo run dev --filter=web",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "codegen:clean": "cd packages && cd api && rm -rf generated",
    "codegen:petstore": "cd packages && cd api && npx openapi-codegen gen petstore",
    "codegen:github": "cd packages && cd api && npx openapi-codegen gen github",
    "codegen:all": "npm run codegen:petstore && npm run codegen:github"
  },
  "devDependencies": {
    "@openapi-codegen/cli": "^2.0.0",
    "@openapi-codegen/typescript": "^8.0.0",
    "@turbo/gen": "^1.12.3",
    "@types/mdx": "^2.0.11",
    "@types/node": "^20.11.17",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-custom": "*",
    "eslint-config-next": "^14.1.0",
    "postcss": "^8.4.31",
    "postcss-preset-env": "^9.3.0",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "sass": "^1.70.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "^3.4.1",
    "turbo": "^1.12.3",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@mdx-js/loader": "^3.0.0",
    "@mdx-js/react": "^3.0.0",
    "@next/mdx": "^14.1.0",
    "@tanstack/react-query": "^5.20.1",
    "class-variance-authority": "^0.7.0",
    "framer-motion": "^10.16.4",
    "glob": "^10.3.10",
    "gray-matter": "^4.0.3",
    "jotai": "^2.5.0",
    "next": "^14.0.0",
    "next-mdx-remote": "^4.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "rehype-highlight": "^7.0.0",
    "rehype-slug": "^6.0.0",
    "rehype-stringify": "^10.0.0",
    "remark-gfm": "^3.0.1"
  }
}
