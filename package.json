{
  "private": true,
  "name": "maxtack",
  "packageManager": "npm@9.2.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "start:portfolio": "turbo run start --filter=portfolio",
    "dev": "turbo run dev",
    "dev:web": "turbo run dev --filter=web",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "codegen:clean": "cd packages && cd api && rm -rf generated",
    "codegen:petstore": "cd packages && cd api && npx openapi-codegen gen petstore",
    "codegen:github": "cd packages && cd api && npx openapi-codegen gen github",
    "codegen:rewards": "cd packages && cd api && npx openapi-codegen gen rewards",
    "codegen:all": "npm run codegen:petstore && npm run codegen:github && npm run codegen:rewards"
  },
  "devDependencies": {
    "@openapi-codegen/cli": "^3.1.0",
    "@openapi-codegen/typescript": "^11.0.1",
    "@turbo/gen": "^2.5.2",
    "@types/mdx": "^2.0.11",
    "@types/node": "^22.15.3",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.2",
    "eslint-config-prettier": "^10.1.2",
    "eslint-config-turbo": "^2.5.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-turbo": "^2.5.0",
    "postcss": "^8.5.3",
    "postcss-preset-env": "^10.2.1",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "sass": "^1.87.0",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^3.4.1",
    "turbo": "^2.5.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@mdx-js/loader": "^3.1.0",
    "@mdx-js/react": "^3.1.0",
    "@next/mdx": "^14.2.2",
    "@tanstack/react-query": "^5.75.2",
    "class-variance-authority": "^0.7.0",
    "framer-motion": "^12.9.4",
    "glob": "^11.0.0",
    "gray-matter": "^4.0.3",
    "jotai": "^2.12.3",
    "next": "^14.2.2",
    "next-mdx-remote": "^4.4.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "rehype-highlight": "^7.0.0",
    "rehype-slug": "^6.0.0",
    "rehype-stringify": "^10.0.0",
    "remark-gfm": "^3.0.1"
  }
}
